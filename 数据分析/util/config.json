  "algorithm_scope": {
    "//": "入选条件，即每个排行榜都有一个入选条件，只有符合条件的人才可以使用algorithm_formula中的公式进行计算,其中的quality指的是回答的质量，计算公式是：总被点赞数/回答数量(数组的第一个为下限，第二个为上限)",
    "//": "全站热榜（高质量大v）",
    "hot_list": {
      "level": 7,
      "answer_count": 100,
      "quality": [
        500,
        5000
      ],
      "voteup_count": 500000,
      "follower_count": 50000
    },
    "//": "水逼榜单（回答多，但得到的赞少，这种人就跟百度贴吧的水逼一样）",
    "water_list": {
      "level": 8,
      "answer_count": 5000,
      "quality": [
        50,
        500
      ],
      "voteup_count": 250000,
      "follower_count": 0
    },
    "//": "新秀榜单（回答质量高，但刚开始玩知乎，还没有粉丝，给予新人一定的曝光）",
    "new_list": {
      "level": 5,
      "answer_count": 100,
      "quality": [
        400,
        5000
      ],
      "voteup_count": 400,
      "follower_count": 1000
    },
    "//": "高质量榜（这里都是回答质量超高的那种人，有营销号或刷赞嫌疑【如：50个回答，但又150w赞的那种人，平均下来每个回答的赞的数量都大于了3w，质量甚至比大v都牛】）",
    "quality_list": {
      "level": 0,
      "answer_count": 100,
      "quality": [
        5000,
        50000000
      ],
      "voteup_count": 10000,
      "follower_count": 0
    }
  },
  "algorithm_formula": {
    "//": "算法公式，用来计算各个排行榜的排名",
    "hot_list": "0.1*{follower_count}+0.35*{product_count}+0.35*{voteup_count}+0.2*{quality}",
    "water_list": "0.7*{answer_count}+0.3*{quality}",
    "new_list": "0.3*{voteup_count}+0.2*{answer_count}+0.5*{quality}",
    "quality_list": "{quality}"
  }
